<h1>  Bands  </h1>


<% @bands.each do |band| %>
  <ul>
    <h2> <%= band.name %> </h2>
    <a href=" <%= edit_band_url(band) %> ">Edit</a>
    <a href=" <%= band_url(band) %> ">See more</a>
  </ul>
<% end %>


<form action=" <%= new_band_url %> " method="post">
  <input type="hidden" name="_method" value="get">
  <input type="submit" value="Add new band">
</form>
